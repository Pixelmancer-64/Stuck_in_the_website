---
import BaseLayout from "../../layouts/BaseLayout.astro";
---

<BaseLayout title="Waves | Pixelmancer" description="I want to find out">
  <div class="asciiDiv">
    <canvas id="canvas"></canvas>

    <div class="controls">
      <label for="resolution" id="resolutionL">Resolution: 5 px</label>
      <input
        type="range"
        id="resolution"
        name="resolution"
        min="1"
        max="20"
        value="10"
      />
    </div>
  </div>

  <script type="module" src="/js/generative-art/ascii.js"
  ></script>
</BaseLayout>
